{
  "name": "snap-doc",
  "version": "0.0.0-development",
  "main": "index.js",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "7.4.0",
    "@commitlint/config-conventional": "7.3.1",
    "@commitlint/travis-cli": "7.4.0",
    "@ember/jquery": "0.6.0",
    "@ember/optional-features": "0.7.0",
    "@types/chai": "4.1.7",
    "@types/debug": "0.0.31",
    "@types/ember": "3.0.26",
    "@types/ember-data": "3.1.3",
    "@types/ember-qunit": "3.4.5",
    "@types/ember-test-helpers": "1.0.4",
    "@types/ember-testing-helpers": "0.0.3",
    "@types/ember__test-helpers": "0.7.7",
    "@types/execa": "0.9.0",
    "@types/mocha": "5.2.5",
    "@types/qunit": "2.5.4",
    "@types/rsvp": "4.0.2",
    "babel-eslint": "10.0.1",
    "broccoli-asset-rev": "3.0.0",
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "debug": "4.1.1",
    "ember-ajax": "4.0.2",
    "ember-cli": "3.7.1",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-code-coverage": "1.0.0-beta.8",
    "ember-cli-dependency-checker": "3.1.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-fastboot": "2.0.4",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-sass": "9.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-typescript-blueprints": "2.0.0-beta.1",
    "ember-cli-uglify": "2.1.0",
    "ember-data": "3.7.0",
    "ember-decorators": "5.1.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.2.0",
    "ember-resolver": "5.0.1",
    "ember-source": "3.7.2",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "ember-welcome-page": "3.2.0",
    "eslint-plugin-ember": "6.2.0",
    "eslint-plugin-node": "8.0.1",
    "husky": "1.3.1",
    "json-typescript": "1.1.0",
    "lerna": "3.10.7",
    "loader.js": "4.7.0",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "nyc": "13.1.0",
    "prember": "1.0.2",
    "qunit-dom": "0.8.4",
    "remark-cli": "6.0.1",
    "remark-lint": "6.0.4",
    "remark-lint-no-dead-urls": "0.4.1",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2",
    "remark-validate-links": "8.0.0",
    "sass": "1.16.1",
    "scripty": "1.9.0",
    "sparkles-component": "1.3.0",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.2.4",
    "typescript-eslint-parser": "22.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        288
      ]
    }
  },
  "scripts": {
    "publish": "scripty",
    "clean": "scripty",
    "build": "lerna run link && lerna run build",
    "build:core": "lerna run link && lerna run build --ignore=@snap-doc/ui-kit --ignore=@snap-doc/app",
    "lint": "lerna run lint --parallel",
    "test": "lerna run test --concurrency 1",
    "test:core": "lerna run test --parallel --ignore=@snap-doc/ui-kit --ignore=@snap-doc/app",
    "rebuild": "scripty",
    "build:watch:core": "scripty",
    "build:watch:all": "scripty",
    "cli:build": "lerna run build --scope=@snap-doc/cli",
    "cli:run": "lerna exec ./bin/snap-doc --scope=@snap-doc/cli",
    "link:code-to-json": "yarn link @code-to-json/core && yarn link @code-to-json/formatter && yarn link @code-to-json/utils && yarn link @code-to-json/utils-ts && yarn link @code-to-json/utils-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snap-doc/snap-doc.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "toolchain": {
    "node": "10.12.0",
    "yarn": "1.10.0"
  }
}
