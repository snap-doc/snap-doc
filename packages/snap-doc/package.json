{
  "name": "snap-doc",
  "version": "0.4.39",
  "description": "> TODO: description",
  "author": "Mike North <michael.l.north@gmail.com>",
  "homepage": "https://github.com/snap-doc/snap-doc#readme",
  "license": "BSD-2-Clause",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snap-doc/snap-doc.git"
  },
  "scripts": {
    "build": "cp ../../README.md README.md",
    "test": "mocha test/**/*.test.ts",
    "lint:js": "eslint . --ext js,ts",
    "lint:md": "remark .",
    "lint": "yarn lint:js && yarn lint:md"
  },
  "bugs": {
    "url": "https://github.com/snap-doc/snap-doc/issues"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "chai": "4.2.0",
    "codecov": "3.2.0",
    "json-typescript": "1.1.0",
    "mocha": "6.0.1",
    "nyc": "13.3.0",
    "scripty": "1.9.1",
    "ts-node": "8.0.3"
  },
  "dependencies": {
    "@mike-north/types": "^1.0.7",
    "@snap-doc/cli": "^0.8.0"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-preset-lint-markdown-style-guide"
    ]
  }
}
