  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snap-doc/snap-doc.git"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:md": "remark .",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:md",
    "start": "ember serve",
    "test": "ember test",
    "test:coverage": "rm -rf coverage && cross-env COVERAGE=true ember test && yarn fix-lcov",
    "codacy:transmit-coverage": "cat coverage/lcov.info | codacy-coverage",
    "coveralls:transmit-coverage": "cat coverage/lcov.info | coveralls",
    "fix-lcov": "scripts/fix-lcov \"$(pwd)\"",
    "try:one": "ember try:one $EMBER_TRY_SCENARIO",
    "prepublish": "ember build --prod"
  },
  "bugs": {
    "url": "https://github.com/snap-doc/snap-doc/issues"
  },
