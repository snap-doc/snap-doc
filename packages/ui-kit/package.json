{
  "name": "@snap-doc/ui-kit",
  "version": "0.4.0",
  "description": "UI-kit for Snap-Doc",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "BSD-2-Clause",
  "author": "Mike North <michael.l.north@gmail.com>",
  "homepage": "https://snap-doc.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:md": "remark .",
    "lint": "yarn lint:js && yarn lint:hbs && yarn lint:md",
    "start": "ember serve",
    "test": "ember test --test-port=8741",
    "test:coverage": "rm -rf coverage && cross-env COVERAGE=true ember test && yarn fix-lcov",
    "codacy:transmit-coverage": "cat coverage/lcov.info | codacy-coverage",
    "coveralls:transmit-coverage": "cat coverage/lcov.info | coveralls",
    "fix-lcov": "scripts/fix-lcov \"$(pwd)\"",
    "test:all": "ember try:each",
    "try:one": "ember try:one $EMBER_TRY_SCENARIO",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@ember-decorators/babel-transforms": "^5.0.0",
    "@mike-north/types": "^1.0.7",
    "ember-cli-babel": "^7.0.0",
    "ember-cli-htmlbars": "3.0.1",
    "ember-cli-typescript": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@types/ember": "3.0.26",
    "@types/ember-data": "3.1.3",
    "@types/ember-qunit": "3.4.5",
    "@types/ember-test-helpers": "1.0.4",
    "@types/ember-testing-helpers": "0.0.3",
    "@types/ember__test-helpers": "0.7.7",
    "@types/qunit": "2.5.4",
    "@types/rsvp": "4.0.2",
    "babel-eslint": "10.0.1",
    "broccoli-asset-rev": "3.0.0",
    "codecov": "3.1.0",
    "cross-env": "5.2.0",
    "ember-cli": "3.7.1",
    "ember-cli-code-coverage": "1.0.0-beta.8",
    "ember-cli-dependency-checker": "3.1.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-typescript-blueprints": "2.0.0-beta.1",
    "ember-cli-uglify": "2.1.0",
    "ember-decorators": "5.1.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.2.0",
    "ember-resolver": "5.0.1",
    "ember-source": "3.7.1",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "6.1.0",
    "eslint-plugin-node": "8.0.1",
    "json-typescript": "1.1.0",
    "loader.js": "4.7.0",
    "nyc": "13.1.0",
    "qunit-dom": "0.8.4",
    "remark-cli": "6.0.1",
    "remark-lint": "6.0.4",
    "remark-lint-no-dead-urls": "0.4.1",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2",
    "remark-validate-links": "8.0.0",
    "scripty": "1.9.0",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.2.4",
    "typescript-eslint-parser": "22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/snap-doc/snap-doc/issues"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-preset-lint-markdown-style-guide"
    ]
  }
}
